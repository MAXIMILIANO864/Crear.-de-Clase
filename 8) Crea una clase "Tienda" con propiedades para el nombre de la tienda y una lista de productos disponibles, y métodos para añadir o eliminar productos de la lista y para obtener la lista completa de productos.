class Tienda:
    def __init__(self, nombre: str):
        self.nombre = nombre
        self.productos = []
    def agregar_producto(self, producto: str):
        self.productos.append(producto)
        return f"Producto '{producto}' añadido a la tienda."
    def eliminar_producto(self, producto: str):
        if producto in self.productos:
            self.productos.remove(producto)
            return f"Producto '{producto}' eliminado de la tienda."
        return f"Producto '{producto}' no encontrado en la tienda."
    def obtener_productos(self):
        return self.productos
if __name__ == "__main__":
    tienda = Tienda("SuperStore")
    print(tienda.agregar_producto("Camisa"))
    print(tienda.agregar_producto("Pantalón"))
    print(tienda.agregar_producto("Zapatos"))
    print("Productos disponibles:", tienda.obtener_productos())
    print(tienda.eliminar_producto("Pantalón"))
    print("Productos disponibles:", tienda.obtener_productos())
